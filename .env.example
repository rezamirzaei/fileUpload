# Environment variables for File Upload Application
# Copy this file to .env and fill in your values
# DO NOT commit .env to git!

# Encryption key for AES-256-GCM (32 bytes, base64 encoded)
# Generate with: openssl rand -base64 32
# WARNING: Without this key, encrypted files cannot be recovered!
ENCRYPTION_SECRET_KEY=

# Database credentials (change in production!)
MYSQL_ROOT_PASSWORD=rootpassword
